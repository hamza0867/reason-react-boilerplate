{
  "name": "reason-react-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "bsb -make-world -clean-world && vite build",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w",
    "re:clean": "bsb -clean-world",
    "extract": "node --experimental-modules --experimental-json-modules src/extractor/index.mjs"
  },
  "dependencies": {
    "@pika/react": "16.13.1",
    "@pika/react-dom": "16.13.1",
    "react-intl": "4.6.9"
  },
  "devDependencies": {
    "bs-platform": "8.0.2",
    "bs-react-intl": "1.1.0",
    "bs-react-intl-extractor-bin": "0.8.0",
    "reason-react": "0.8.0",
    "vite": "1.0.0-rc.4",
    "vite-plugin-react": "3.0.2"
  }
}
